EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/Nimble-iOS" "$CONFIGURATION_BUILD_DIR/OpenShop-iOS" "$CONFIGURATION_BUILD_DIR/Quick-iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Nimble-iOS/Nimble.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/OpenShop-iOS/OpenShop.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Quick-iOS/Quick.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Nimble" -framework "OpenShop" -framework "Quick"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}/Pods
